record (bi, "s7-status") {
  field (DTYP, "S7plc stat")
  field (INP,  "@Siemens_S7: 0")
  field (SCAN, ".5 second")
  field (ZNAM, "disconnected")
  field (ONAM, "connected")
  field (ZSV,  "MAJOR")
}

record (ai, "ai-int16-1") {
  # map range 0 ... 1000 to 0.0000 ... 10.0000
  field (DTYP, "S7plc")
  field (INP,  "@Siemens_S7: 0/0 'T=INT16 L=0 H=1000'")
  field (EGUF, "10")
  field (LOPR, "-10")
  field (HOPR, "10")
  field (LINR, "LINEAR")
  field (PREC, "2")
  field (SCAN, ".5 second")
}

